<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yutaka.dayrepo.mapper.DairyReportMapper">
	<select id="listCategories" resultType="com.yutaka.dayrepo.model.query.DairyReportQuery">
		SELECT * FROM reports;
	</select>

	<insert id="addDairyReport" parameterType="com.yutaka.dayrepo.model.attribute.DairyReportAttribute">
		INSERT INTO
			dairy_report
		(
			id,
			title,
			tag,
			document,
			comment,
			markdown,
			updated_at
		)
		VALUES
		(
			#{id},
			#{title},
			#{tag},
			#{document, jdbcType=VARCHAR},
			#{comment},
			#{markdown},
			#{updatedAt}
		)
	</insert>
</mapper>